                                <div class="col-sm-12 col-lg-6 col-xl-4 mb-xlg">
                                    <div class="pb-xlg h-100">
                                        <section class="widget mb-0 h-100">
                                            <header class="d-flex justify-content-between flex-wrap">
                                                <h4 class="d-flex align-items-center pb-1 big stat-title">Résultats des tests</h4>
                                            </header>
                                            <div class="widget-body">
                                                <table class="quick-list">
                                                    <tr>
                                                        <td class="meta-data-title">Total</td>
                                                        <td class="result-data"><%= suite.featureCount.total %> features</td>
                                                        <td class="result-data"><%= suite.scenarios.total %> scenarios</td>
                                                        <td class="result-data"><%= suite.steps.total %> <small>steps</small></td>
                                                    </tr>
                                                    <tr>
                                                        <td class="meta-data-title result-passed">Succès</td>
                                                        <td class="result-data"><%= suite.featureCount.passedPercentage %> %</td>
                                                        <td class="result-data"><%= suite.scenarios.passed %> scenarios</td>
                                                        <td class="result-data"><%= suite.steps.passed %> <small>steps</small></td>
                                                    </tr>
                                                    <tr>
                                                        <td class="meta-data-title result-failed">Echecs</td>
                                                        <td class="result-data"><%= suite.featureCount.failedPercentage %> %</td>
                                                        <td class="result-data"><%= suite.scenarios.failed %> scenarios</td>
                                                        <td class="result-data"><%= suite.steps.failed %> <small>steps</small></td>
                                                    </tr>
                                                    <tr>
                                                        <td class="meta-data-title result-skipped">Non exécutés</td>
                                                        <td class="result-data"><%= suite.featureCount.skippedPercentage %> %</td>
                                                        <td class="result-data"><%= suite.scenarios.skipped %> scenarios</td>
                                                        <td class="result-data"><%= suite.steps.skipped %> <small>steps</small></td>
                                                    </tr>
                                                    <tr>
                                                        <td class="meta-data-title result-pending">Ignorés</td>
                                                        <td class="result-data"><%= suite.featureCount.pendingPercentage %> %</td>
                                                        <td class="result-data"><%= suite.scenarios.pending %> scenarios</td>
                                                        <td class="result-data"><%= suite.steps.pending %> <small>steps</small></td>
                                                    </tr>
                                                    <% if (suite.scenarios.notDefined !=0) { %>
                                                    <tr>
                                                        <td class="meta-data-title result-notDefined">Non définis</td>
                                                        <td class="result-data"><%= suite.featureCount.notdefinedPercentage %> %</td>
                                                        <td class="result-data"><%= suite.scenarios.notDefined %> scenarios</td>
                                                        <td class="result-data"><%= suite.steps.notDefined %> <small>steps</small></td>
                                                    </tr>
                                                    <% } %>
                                                </table>
                                            </div>
                                        </section>
                                    </div>
                                </div>
                                <div class="col-sm-12 col-lg-6 col-xl-4 mb-xlg">
                                    <div class="pb-xlg h-100">
                                        <%= customDataOverview %>
                                    </div>
                                </div>
                                <div class="col-md-12 col-sm-12 col-xs-12">
                                    <div class="widget pb-0">
                                        <header>
                                            <h4>Aperçu des tests en échec</h4>
                                        </header>
                                        <div class="widget-body p-0 support table-wrapper">
                                        <% if (suite.featureCount.passedPercentage !=100) { %>
                                            <table class="table table-striped mb-0">
                                                <thead>
                                                    <tr class="text-muted">
                                                        <th scope="col">
                                                            <span class="pl-3">Feature Name</span>
                                                        </th>
                                                        <th>Tags</th>
                                                        <th>Status</th>
                                                        <% if (!suite.hideMetadata) { %>
                                                            <th>Device</th>
                                                            <th>OS</th>
                                                            <% if(+suite.app > 0) { %>
                                                            <th>App</th>
                                                            <% } %>
                                                            <% if(+suite.browser > 0) { %>
                                                            <th>Browser</th>
                                                            <% } %>
                                                        <% } %>
                                                        <% if (suite.displayDuration) { %>
                                                        <th scope="col">Duration</th>
                                                        <% } %>
                                                    </tr>
                                                </thead>
                                                <tbody class="text-dark">
                                                    <% _.each(suite.features, function(feature, featureIndex) { %>
                                                    <% if (feature.isFailed || feature.isAmbiguous || feature.isBlocked || feature.isPending || feature.isSkipped || feature.isNotdefined) { %>
                                                        <tr>
                                                        <% var colNumber=3; %>
                                                            <td class="pl-4 fw-normal"><%= feature.name %> <p class="featureURI"><%= feature.uri %></p></td>
                                                            <td class="text-center">
                                                                <% if (feature.tags) { %>
                                                                <% var amount = feature.tags.length; %>
                                                                <% var tags = _.reduce(feature.tags, (tags, tag) => tags + tag.name + ' ', ''); %>
                                                                <% if (amount > 0 ){ %>
                                                                    <span><%= tags.trim() %></span>
                                                                <% } %>
                                                                <% } %>
                                                            </td>
                                                            <td class="text-center">
                                                                <% if (feature.isFailed) { %>
                                                                <span class="btn btn-xs p-1 px-3 btn-failed">Failed</span>
                                                                <% } else if (feature.isAmbiguous) { %>
                                                                <span class="btn btn-xs p-1 px-3 btn-ambigous">Ambigous</span>
                                                                <% } else if (feature.isBlocked) { %>
                                                                <span class="btn btn-xs p-1 px-3 btn-blocked">Blocked</span>
                                                                <% } else if (feature.isPending) { %>
                                                                <span class="btn btn-xs p-1 px-3 btn-pending">Pending</span>
                                                                <% } else if (feature.isSkipped) { %>
                                                                <span class="btn btn-xs p-1 px-3 btn-skipped">Skipped</span>
                                                                <% } else if (feature.isNotdefined) { %>
                                                                <span class="btn btn-xs p-1 px-3 btn-undefined">Not defined</span>
                                                                <% } else { %>
                                                                <span class="btn btn-xs p-1 px-3 btn-passed">Passed</span>
                                                                <% } %>
                                                            </td>
                                                            <% if (!suite.hideMetadata) { %>
                                                            <% var colNumber=colNumber+2; %>
                                                                <td>
                                                                    <% var device; %>
                                                                    <% if (feature.metadata.device === 'not known' || feature.metadata.device === '') { %>
                                                                    <% device = '<i class="fa question-circle fa-lg"></i> not known'; %>
                                                                    <% } else { %>
                                                                    <% device = feature.metadata.device; %>
                                                                    <% } %>
                                                                    <%= device %>
                                                                </td>
                                                                <td>
                                                                    <% var platformName; %>
                                                                    <% if (feature.metadata.platform.name.toLowerCase() === 'ios' || feature.metadata.platform.name.toLowerCase() === 'osx') { %>
                                                                    <% platformName = 'apple'; %>
                                                                    <% } else if (feature.metadata.platform.name.toLowerCase() === 'android') { %>
                                                                    <% platformName = 'android'; %>
                                                                    <% } else if (feature.metadata.platform.name.toLowerCase() === 'windows') { %>
                                                                    <% platformName = 'windows'; %>
                                                                    <% } else if (feature.metadata.platform.name.toLowerCase() === 'ubuntu' ||
                                                                    feature.metadata.platform.name.toLowerCase() === 'linux') { %>
                                                                    <% platformName = 'linux'; %>
                                                                    <% } else { %>
                                                                    <% platformName = ' (?) '; %>
                                                                    <% } %>
                                                                    <%= platformName %>
                                                                    <%= feature.metadata.platform.version %>
                                                                </td>
                                                                <% if(+suite.app > 0) { %>
                                                                <% var colNumber=colNumber+1; %>
                                                                <td>
                                                                    <% if(feature.metadata.app) { %>
                                                                    <%= feature.metadata.app.name %> <br>V: <%= feature.metadata.app.version %>
                                                                    <% } %>
                                                                </td>
                                                                <% } %>
                                                                <% if(+suite.browser > 0) { %>
                                                                <% var colNumber=colNumber+1; %>
                                                                <td>
                                                                    <% if(feature.metadata.browser) { %>
                                                                    <%= feature.metadata.browser.name.toLowerCase() %>
                                                                    <%= feature.metadata.browser.version %>
                                                                    <% } %>
                                                                </td>
                                                                <% } %>
                                                            <% } %>
                                                            <% if (suite.displayDuration) { %>
                                                            <% var colNumber=colNumber+1; %>
                                                            <td class="duration"><%= feature.time %></td>
                                                            <% } %>
                                                        </tr>
                                                        <tr>
                                                            <td colspan="<%= colNumber %>">
                                                                <div class="row">
                                                                    <ul style="margin: 0 20px 0 20px;padding: 0 20px 0 20px; width:100%">
                                                                        <header>
                                                                            <% if (feature.isFailed || feature.isAmbiguous || feature.isBlocked || feature.isPending || feature.isSkipped || feature.isNotdefined) { %>
                                                                            <details open>
                                                                            <% } else { %>
                                                                            <details>
                                                                            <% } %>
                                                                                <div class="col-md-12 col-sm-12 col-xs-12">
                                                                                    <ul class="test--list">
                                                                                    <% _.each(feature.elements, function(scenario, scenarioIndex) { %>
                                                                                        <li class="test--component">
                                                                                            <header>
                                                                                            <% if (scenario.isFailed || scenario.isAmbiguous || scenario.isBlocked || scenario.isPending || scenario.isSkipped || feature.isNotdefined) { %>
                                                                                            <details open>
                                                                                            <% } else { %>
                                                                                            <details>
                                                                                            <% } %>
                                                                                            <summary><span><%= scenario.keyword %>: <%= scenario.name %></span></summary>
                                                                                                <div class="x_panel" style="height: auto;">
                                                                                                    <div class="x_title">
                                                                                                        <div class="tags">
                                                                                                            <% if (scenario.tags) { %>
                                                                                                                <% var amount = scenario.tags.length; %>
                                                                                                                <% if (amount > 0 ){ %>
                                                                                                                    <i class="fa fa-tag<% if(amount > 1 ) {%>s<% } %> fa-lg"></i>
                                                                                                                    <% _.each(scenario.tags, (tag) => {%>
                                                                                                                        <span class="tag"><%= tag.name %></span>
                                                                                                                    <%}); %>
                                                                                                                <% } %>
                                                                                                            <% } %>
                                                                                                        </div>
                                                                                                        
                                                                                                    </div>
                                                                                                    <div class="x_content" >
                                                                                                        <% _.each(scenario.steps, function(step, stepIndex) { %>
                                                                                                            <% if(!step.hidden || step.image || step.text || step.html || step.attachment) { %>
                                                                                                                <div class="scenario-step-container">
                                                                                                                    <% if(step.result) { %>
                                                                                                                        <% var statusStep; %>
                                                                                                                        <% if(step.result.status === 'passed') { statusStep="passed"
                                                                                                                        } else if(step.result.status === 'failed') { statusStep="failed"
                                                                                                                        } else if(step.result.status === 'skipped') { statusStep="skipped"
                                                                                                                        } else if(step.result.status === 'pending') { statusStep="pending"
                                                                                                                        } else if(step.result.status === 'ambiguous') { statusStep="ambigous"
                                                                                                                        } else { statusStep="undefined"
                                                                                                                        } %>
                                                                                                                    <% } %>

                                                                                                                    <div class="text">
                                                                                                                        <span class="keyword highlight <%= statusStep %>"><%= step.keyword %></span>

                                                                                                                        <% if(step.name) { %>
                                                                                                                            <%= step.name.replace(/</g, '&lt;').replace(/>/g, '&gt;') %>
                                                                                                                            <% if(step.restWireData) { %>
                                                                                                                                <button type="button" class="btn btn-success btn-sm" data-toggle="collapse" data-target="#<%= step.id%>">REST Query</button>
                                                                                                                                <div id="<%= step.id%>" class="collapse">
                                                                                                                                    <div class="panel panel-default">
                                                                                                                                        <div class="panel-body">
                                                                                                                                            <pre><%= step.restWireData %></pre>
                                                                                                                                        </div>
                                                                                                                                    </div>
                                                                                                                                </div>
                                                                                                                            <% } %>
                                                                                                                        <% } %>

                                                                                                                        <% if(suite.displayDuration) { %>
                                                                                                                            <% if (step.time) { %>
                                                                                                                                <span class="duration"><%= step.time %></span>
                                                                                                                            <% } else { %>
                                                                                                                                <span class="duration">0s</span>
                                                                                                                            <% } %>
                                                                                                                        <% } %>
                                                                                                                    </div>
                                                                                                                    <% if(step.result) { %>
                                                                                                                        <% if(step.result.status === 'undefined') { %>
                                                                                                                            <pre class="info">
                                                                                                                            // With Callbacks
                                                                                                                            <%=step.keyword%>(/^<%= step.name.replace(/"[^"]*"/g, '"\(\[\^\"\]\*\)"') %>$/, (<% for (var i=1; i<(step.name.split('"').length / 2); i++) { %>arg<%= i %>, <% } %>callback) => {
                                                                                                                                // Write code here that turns the phrase above into concrete actions
                                                                                                                                callback(null, 'pending');
                                                                                                                            });

                                                                                                                            // With Promises
                                                                                                                            <%=step.keyword%>(/^<%= step.name.replace(/"[^"]*"/g, '"\(\[\^\"\]\*\)"') %>$/, (<% for (var i=1; i<(step.name.split('"').length / 2); i++) { %>arg<%= i %>, <% } %>) => {
                                                                                                                                // Write code here that turns the phrase above into concrete actions
                                                                                                                                return Promise.resolve('pending');
                                                                                                                            });
                                                                                                                            </pre>
                                                                                                                        <% } %>

                                                                                                                    <% } %>
                                                                                                                </div>
                                                                                                            <% } %>
                                                                                                            <% if(step.result) { %>
                                                                                                                <% if (step.result.error_message) { %>
                                                                                                                    <div id="error<%= scenarioIndex %><%= stepIndex %>-error" class="test--code-snippet">
                                                                                                                        <pre class=info><%= step.result.error_message.replace(/</g, '(').replace(/>/g, ')') %></pre>
                                                                                                                    </div>
                                                                                                                <% } %>

                                                                                                                <% if (step.json) { %>
                                                                                                                    <div id="info<%= scenarioIndex %><%= stepIndex %>-text" class="test--code-snippet">
                                                                                                                        <pre class=info><% try { %><%= JSON.stringify(step.json, undefined, 2) %><% } catch (error) { %><%= step.json %><% } %></pre>
                                                                                                                    </div>
                                                                                                                <% } %>

                                                                                                                <% if (step.text) { %>
                                                                                                                    <div id="info<%= scenarioIndex %><%= stepIndex %>-text" class="test--code-snippet">
                                                                                                                        <pre class=info><%= step.text.join('<br>') %></pre>
                                                                                                                    </div>
                                                                                                                <% } %>

                                                                                                                <% if (step.html) { %>
                                                                                                                    <div id="info<%= scenarioIndex %><%= stepIndex %>-html" class="test--code-snippet">
                                                                                                                        <div class=info><%= step.html.join('<br/>') %></div>
                                                                                                                    </div>
                                                                                                                <% } %>

                                                                                                                <% if (step.image) { %>
                                                                                                                    <div id="info<%= scenarioIndex %><%= stepIndex %>-image" class="test--code-snippet">
                                                                                                                        <% for( var i = 0; i < step.image.length; i++ ) { %>
                                                                                                                            <img class="screenshot" src="<%= step.image[i] %>"/>
                                                                                                                        <% } %>
                                                                                                                    </div>
                                                                                                                <% } %>

                                                                                                                <% if (!_.isEmpty(step.attachments)) { %>
                                                                                                                    <% _.each(step.attachments, function(attachment, attachmentIndex) { %>
                                                                                                                        <div id="info<%= scenarioIndex %><%= stepIndex %>-attachment<%= attachmentIndex %>" class="test--body">
                                                                                                                            <object class="attachment-object" type="<%= attachment.type %>" id="my_attachments_<%= attachmentIndex %>" data="<%= attachment.data %>"></object>
                                                                                                                        </div>
                                                                                                                    <% }); %>
                                                                                                                <% } %>
                                                                                                            <% } %>
                                                                                                            <% if(step.arguments) { %>
                                                                                                                <% for( var i = 0; i < step.arguments.length; i++ ) { %>
                                                                                                                    <div class="scenario-scroll-bar">
                                                                                                                        <% if(step.arguments[i]["rows"]) { %>
                                                                                                                            <table class="table arguments">
                                                                                                                                <% var rows = step.arguments[i]["rows"]; %>
                                                                                                                                <tr>
                                                                                                                                    <% var cells = rows[0]["cells"]; %>
                                                                                                                                    <% for( var k = 0; k < cells.length; k++ ) { %>
                                                                                                                                        <th><%= cells[k] %></th>
                                                                                                                                    <% } %>
                                                                                                                                </tr>

                                                                                                                                <% for( var j = 1; j < rows.length; j++ ) { %>
                                                                                                                                    <tr>
                                                                                                                                        <% var cells = rows[j]["cells"]; %>
                                                                                                                                        <% for( var k = 0; k < cells.length; k++ ) { %>
                                                                                                                                            <td><%= cells[k] %></td>
                                                                                                                                        <% } %>
                                                                                                                                    </tr>
                                                                                                                                <% } %>
                                                                                                                            </table>
                                                                                                                        <% } %>
                                                                                                                    </div>

                                                                                                                    <% if(step.arguments[i]["content"]) { %>
                                                                                                                        <pre class="info"><%= step.arguments[i]["content"].replace(/</g, '(').replace(/>/g, ')') %></pre>
                                                                                                                    <% } %>
                                                                                                                <% } %>
                                                                                                            <% } %>
                                                                                                        <% }); %>
                                                                                                    </div>
                                                                                                </div>
                                                                                            </details>
                                                                                            </header>
                                                                                        </li>
                                                                                    <% }); %>
                                                                                    </ul>
                                                                                </div>
                                                                            </details>
                                                                        </header>
                                                                    </ul>
                                                                </div>
                                                            </td>
                                                        </tr>
                                                    <% } %>
                                                    <% }); %>
                                                </tbody>
                                            </table>
                                        <% } else { %>
                                            <div style="text-align:center"><p>Pas de tests en échec</p><p class="trophy"></p></div>
                                        <% } %>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    
            <footer class="content-footer">
        </main>
    <div>
</body>
</html>
